{
  "name": "sample", // devcontainer name
  "service": "sample-service", // service name in docker-compose.yml
  "workspaceFolder": "/app", // WORKDIR in Dockerfile
  "dockerComposeFile": "../../compose.yml", // docker-compose.yml file path
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "vscode.typescript-language-features",
        "vscode.css-language-features",
        "vscode.html-language-features",
        "vscode.json-language-features",
        "vscode.markdown-language-features",
        "vscode.yaml-language-features"
      ]
    }
  }
}
